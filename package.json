{
  "name": "update-dependencies",
  "version": "1.0.0",
  "description": "GitHub Action for staying up to date on the latest dependencies",
  "author": "Brian Buchanan",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/bpbuch/update-dependencies#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpbuch/update-dependencies.git"
  },
  "bugs": {
    "url": "https://github.com/bpbuch/update-dependencies/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "all": "npm run build && npm run pack",
    "build": "tsc",
    "pack": "ncc build",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.1",
    "npm-registry-fetch": "^7.0.0",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@types/npm": "^2.0.31",
    "@types/npm-registry-fetch": "^4.0.1",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "typescript": "^3.8.2"
  }
}
